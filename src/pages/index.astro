  keywords="inmobiliaria, propiedades, casas, departamentos, alquiler, venta"
  opDescription="Una empresa del grupo POSBEN"
  opImage="/meta/pbii-op.webp"
>
  <main>
    <section class="mx-auto w-full py-10 lg:w-[740px]">
